   "source": [
    "def convert3(obj):\n",
    "    L=[]\n",
    "    cnt=0\n",
    "    for i in ast.literal_eval(obj):\n",
    "        if cnt!=3:\n",
    "          L.append(i['name'])\n",
    "          cnt+=1\n",
    "        else:\n",
    "            break\n",
    "    return L"
   ]
  },
