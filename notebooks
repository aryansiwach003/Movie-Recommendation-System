    "def fetch_director(obj):\n",
    "    L=[]\n",
    "    for i in ast.literal_eval(obj):\n",
    "        if i['job']=='Director':\n",
    "          L.append(i['name'])\n",
    "          break\n",
    "    return L"
   ]
